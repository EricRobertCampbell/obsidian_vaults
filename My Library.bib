  shorttitle = {Phylogeny of {{Allosauroidea}} ({{Dinosauria}}},
  author = {Brusatte, Stephen L. and Sereno, Paul C.},
  date = {2008-01-01},
  journaltitle = {Journal of Systematic Palaeontology},
  volume = {6},
