  shorttitle = {A {{New Era}} of {{Morphological Investigations}}},
  author = {Ford, K L and Albert, J S and Summers, A P and Hedrick, B P and Schachner, E R and Jones, A S and Evans, K and Chakrabarty, P},
  date = {2023-03-17},
  journaltitle = {Integrative Organismal Biology},
