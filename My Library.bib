@article{chinsamyGenderIdentificationMesozoic2013,
  title = {Gender Identification of the {{Mesozoic}} Bird {{Confuciusornis}} Sanctus},
  author = {Chinsamy, Anusuya and Chiappe, Luis M. and Marugán-Lobón, Jesús and Chunling, Gao and Fengjiao, Zhang},
  date = {2013-01-22},
