}

@article{rogersCannibalismMadagascanDinosaur2003,
  title = {Cannibalism in the {{Madagascan}} Dinosaur {{Majungatholus}} Atopus},
  author = {Rogers, Raymond R. and Krause, David W. and Curry Rogers, Kristina},
  date = {2003-04},
  journaltitle = {Nature},
