@article{knappThreedimensionalGeometricMorphometric2021,
  title = {Three-Dimensional Geometric Morphometric Analysis of the Skull of {{Protoceratops}} Andrewsi Supports a Socio-Sexual Signalling Role for the Ceratopsian Frill},
  author = {Knapp, A. and Knell, R. J. and Hone, D. W. E.},
  date = {2021-02-03},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {288},
  number = {1944},
  pages = {20202938},
  publisher = {Royal Society},
  doi = {10.1098/rspb.2020.2938},
  url = {https://royalsocietypublishing.org/doi/full/10.1098/rspb.2020.2938},
  urldate = {2023-05-08},
  abstract = {Socio-sexual selection is predicted to be an important driver of evolution, influencing speciation, extinction and adaptation. The fossil record provides a means of testing these predictions, but detecting its signature from morphological data alone is difficult. There are, nonetheless, some specific patterns of growth and variation which are expected of traits under socio-sexual selection. The distinctive parietal-squamosal frill of ceratopsian dinosaurs has previously been suggested as a socio-sexual display trait, but evidence for this has been limited. Here, we perform a whole-skull shape analysis of an unprecedentedly large sample of specimens of Protoceratops andrewsi using a high-density landmark-based geometric morphometric approach to test four predictions regarding a potential socio-sexual signalling role for the frill. Three predictions—low integration with the rest of the skull, significantly higher rate of change in size and shape during ontogeny, and higher morphological variance than other skull regions—are supported. One prediction, sexual dimorphism in shape, is not supported, suggesting that sexual differences in P. andrewsi are likely to be small. Together, these findings are consistent with mutual mate choice or selection for signalling quality in more general social interactions, and support the hypothesis that the frill functioned as a socio-sexual signal in ceratopsian dinosaurs.},
  keywords = {!!!,dinosauria,evolution,geometric morphometrics,socio-sexual selection,to-read},
  file = {/Users/ericr/Zotero/storage/GD6L5YXW/Knapp et al. - 2021 - Three-dimensional geometric morphometric analysis .pdf}
}

