@article{gilbertTaphonomyDepositionalHistory2022,
  title = {Taphonomy and Depositional History of the {{Southfork Quarry}} ({{Cypress Hills Formation}}, Late {{Eocene}}) in Southwestern {{Saskatchewan}}, {{Canada}}},
  author = {Gilbert, Meagan M. and McDougall, Frank H.},
  date = {2022-10},
  journaltitle = {Canadian Journal of Earth Sciences},
  shortjournal = {Can. J. Earth Sci.},
  volume = {59},
