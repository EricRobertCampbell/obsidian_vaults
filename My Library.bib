}

@article{hornerDinosaurCensusReveals2011,
  title = {Dinosaur {{Census Reveals Abundant Tyrannosaurus}} and {{Rare Ontogenetic Stages}} in the {{Upper Cretaceous Hell Creek Formation}} ({{Maastrichtian}}), {{Montana}}, {{USA}}},
