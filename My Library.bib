@article{serenoComparativeAssessmentTyrannosaurid2009,
  title = {Comparative {{Assessment}} of Tyrannosaurid Interrelationships},
  author = {Sereno, Paul C. and Brusatte, Stephen L.},
  date = {2009-12-01},
  journaltitle = {Journal of Systematic Palaeontology},
