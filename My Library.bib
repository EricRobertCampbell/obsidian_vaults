@article{milnerBirdLikeAnatomyPosture2009,
  title = {Bird-{{Like Anatomy}}, {{Posture}}, and {{Behavior Revealed}} by an {{Early Jurassic Theropod Dinosaur Resting Trace}}},
  author = {Milner, Andrew R. C. and Harris, Jerald D. and Lockley, Martin G. and Kirkland, James I. and Matthews, Neffra A.},
