@article{ericksonGigantismComparativeLifehistory2004,
  title = {Gigantism and Comparative Life-History Parameters of Tyrannosaurid Dinosaurs},
  author = {Erickson, Gregory M. and Makovicky, Peter J. and Currie, Philip J. and Norell, Mark A. and Yerby, Scott A. and Brochu, Christopher A.},
  date = {2004-08},
  journaltitle = {Nature},
  volume = {430},
  number = {7001},
  pages = {772--775},
  publisher = {Nature Publishing Group},
  issn = {1476-4687},
  doi = {10.1038/nature02699},
  url = {https://www.nature.com/articles/nature02699},
  urldate = {2022-07-02},
  abstract = {How evolutionary changes in body size are brought about by variance in developmental timing and/or growth rates (also known as heterochrony) is a topic of considerable interest in evolutionary biology1. In particular, extreme size change leading to gigantism occurred within the dinosaurs on multiple occasions2. Whether this change was brought about by accelerated growth, delayed maturity or a combination of both processes is unknown. A better understanding of relationships between non-avian dinosaur groups and the newfound capacity to reconstruct their growth curves make it possible to address these questions quantitatively3. Here we study growth patterns within the Tyrannosauridae, the best known group of large carnivorous dinosaurs, and determine the developmental means by which Tyrannosaurus rex, weighing 5,000\,kg and more, grew to be one of the most enormous terrestrial carnivorous animals ever. T. rex had a maximal growth rate of 2.1\,kg\,d-1, reached skeletal maturity in two decades and lived for up to 28 years. T. rex's great stature was primarily attained by accelerating growth rates beyond that of its closest relatives.},
  issue = {7001},
  langid = {english},
  keywords = {Humanities and Social Sciences,multidisciplinary,Ontogeny,read,Science},
  file = {/Users/ericr/Zotero/storage/GUNE3MXV/41586_2016_BFnature16487_MOESM2_ESM.zip;/Users/ericr/Zotero/storage/TF48SQLR/Erickson et al. - 2004 - Gigantism and comparative life-history parameters .pdf;/Users/ericr/Zotero/storage/4PZUUIAD/nature02699.html}
}

@article{ericksonHowTerrorCrocodile1999,
  title = {How the ‘Terror Crocodile’ Grew so Big},
  author = {Erickson, Gregory M. and Brochu, Christopher A.},
  date = {1999-03},
  journaltitle = {Nature},
  volume = {398},
  number = {6724},
  pages = {205--206},
  publisher = {Nature Publishing Group},
  issn = {1476-4687},
  doi = {10.1038/18343},
  url = {https://www.nature.com/articles/18343},
  urldate = {2022-11-16},
  abstract = {Deinosuchus is a giant crocodylian from the Late Cretaceous period of North America. It was 8 to 10 metres long and weighed between 2,500 and 5,000 kg, three to five times more than the largest crocodiles alive today. How Deinosuchus attained sizes to rival its dinosaurian contemporaries, on which it undoubtedly preyed, has remained a mystery. Did it exhibit accelerated growth rates, like its dinosaurian cousins1, or did it simply maintain primitive reptilian rates for decades (as was once proposed to explain gigantism in dinosaurs2)? We find that growth indices from Deinosuchus skeletons reveal rates comparable to those of smaller crocodylian taxa, indicating that the gigantic proportions were attained by prolonging development.},
  issue = {6724},
  langid = {english},
  keywords = {Humanities and Social Sciences,multidisciplinary,Science,To Read},
