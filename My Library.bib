@article{honeOntogenySexuallySelected2020,
  title = {Ontogeny of a Sexually Selected Structure in an Extant Archosaur {{Gavialis}} Gangeticus ({{Pseudosuchia}}: {{Crocodylia}}) with Implications for Sexual Dimorphism in Dinosaurs},
  shorttitle = {Ontogeny of a Sexually Selected Structure in an Extant Archosaur {{Gavialis}} Gangeticus ({{Pseudosuchia}}},
  author = {Hone, David and Mallon, Jordan C. and Hennessey, Patrick and Witmer, Lawrence M.},
  date = {2020-05-12},
  journaltitle = {PeerJ},
  shortjournal = {PeerJ},
  volume = {8},
